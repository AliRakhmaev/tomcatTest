<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>MakeAShoutServlet</servlet-name>
    <servlet-class>MakeAShoutServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AIStrikesBackServlet</servlet-name>
    <servlet-class>AIStrikesBackServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PlayerShipDestroyedServlet</servlet-name>
    <servlet-class>PlayerShipDestroyedServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>MakeAShoutServlet</servlet-name>
    <url-pattern>/userHitServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AIStrikesBackServlet</servlet-name>
    <url-pattern>/aiStrikesBackServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PlayerShipDestroyedServlet</servlet-name>
    <url-pattern>/playerShipDestroyedServlet</url-pattern>
  </servlet-mapping>

</web-app>
